<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE Ann-SyntaxHighlighting>
<!-- There are 5 special characters that need to be coded:    -->
<!-- & - &amp;   < - &lt;   > - &gt;   " - &quot;   ' - &apos; -->
<Ann-SyntaxHighlighting>
   <Syntax>
    <Name>LaTeX</Name>
    <Qualifying>
      <AndTokens>\documentclass \begin</AndTokens>
      <OrTokens min="10"> { } \begin \end \documentclass \documentstyle \newcommand \usepackage</OrTokens> 
      <FilePattern>#?.(tex|cls|sty|dtx)</FilePattern>
    </Qualifying>
    <Tokens>70</Tokens>
    <!--math symbols: bg=3 fg=2, document stuff: bold, environment: bg=2 fg=3, sectioning: underline, font commands: fg=3+bold-->
    <!--parenteses: bg=3+fg=2, referencing+citations bgpen=1 fgpen=2, lists: ng=3,fg=2-->
    <Token style="bold">\documentclass</Token>
    <Token style="bold">\usepackage</Token>
    <Token style="bold">\newcommand</Token>
    <Token style="bold">\let</Token>
    <Token style="bold">\def</Token>
    <Token style="bold">\begin</Token>
    <Token style="bold">\end</Token>
    <Token style="bold" bgpen="3">{document}</Token>
    <Token bgpen="2" pen="3">{split}</Token>
    <Token bgpen="2" pen="3">{multline}</Token>
    <Token bgpen="2" pen="3">{gather}</Token>
    <Token bgpen="2" pen="3">{align}</Token>
    <Token bgpen="2" pen="3">{flalign}</Token>
    <Token bgpen="2" pen="3">{eqnarray}</Token>
    <Token bgpen="2" pen="3">{equation}</Token>
    <Token bgpen="2" pen="3">{tabular}</Token>
    <Token bgpen="2" pen="3">{table}</Token>
    <Token bgpen="2" pen="3">{figure}</Token>
    <Token bgpen="2" pen="3">{align*}</Token>
    <Token bgpen="2" pen="3">{flalign*}</Token>
    <Token bgpen="2" pen="3">{eqnarray*}</Token>
    <Token bgpen="2" pen="3">{equation*}</Token>
    <Token bgpen="2" pen="3">{tabular*}</Token>
    <Token bgpen="2" pen="3">{table*}</Token>
    <Token bgpen="2" pen="3">{figure*}</Token>
    <Token bgpen="2" pen="3">{matrix}</Token>
    <Token bgpen="2" pen="3">{array}</Token>
    <Token bgpen="3" pen="2">{enumerate}</Token>
    <Token bgpen="3" pen="2">{itemize}</Token>
    <Token bgpen="3" pen="2">{list}</Token>
    <Token bgpen="3" pen="2">\item</Token>
    <Token style="underline" pen="3">\sethebrew</Token>
    <Token style="underline" pen="3">\unsethebrew</Token>
    <Token style="underline" pen="3">\large</Token>
    <Token style="underline" pen="3">\Large</Token>
    <Token style="underline" pen="3">\LARGE</Token>
    <Token style="underline" pen="3">\huge</Token>
    <Token style="underline" pen="3">\Huge</Token>
    <Token style="underline" pen="3">\small</Token>
    <Token style="underline" pen="3">\tiny</Token>
    <Token style="underline" pen="3">\ns</Token>
    <Token style="underline" pen="3">\normalsize</Token>
    <Token style="underline" pen="4">\chapter</Token>
    <Token style="underline" pen="4">\section</Token>
    <Token style="underline" pen="4">\subsection</Token>
    <Token style="underline" pen="4">\subsubsection</Token>
    <!--Consts-->
    <Token pen="14">equation</Token>
    <Token pen="14">eqnarray</Token>
    <Token pen="14">table</Token>
    <Token pen="14">figure</Token>
    <Token style="bold" pen="2">{</Token>
    <Token style="bold" pen="2">}</Token>
    <Token bgpen="3" pen="2">[</Token>
    <Token bgpen="3" pen="2">]</Token>
    <!--String and char-const tokens:-->
    <!--Comments...-->
    <ComplexToken>
      <Token pen="2" >%</Token>
      <MatchToEOL/>
    </ComplexToken>
    <ComplexToken>
      <Token bgpen="13">\begin{figure}</Token>
      <MatchToToken>\end{figure}</MatchToToken>
      <MatchToEOL/>
    </ComplexToken>
    <ComplexToken>
      <Token bgpen="3">\begin{align}</Token>
      <MatchToToken>\end{align}</MatchToToken>
      <Multiline/>
    </ComplexToken>
 <!--   <Token style="bold">\begin</Token>
    <Token style="bold">\end</Token>-->
    <!--math environment...-->
    <ComplexToken>
      <Token pen="3" bgpen="2">$$</Token>
      <MatchToToken>$$</MatchToToken>
      <Mutliline/>
    </ComplexToken>
    <ComplexToken>
      <Token pen="12">\L{</Token>
      <MatchToToken>}</MatchToToken>
      <MultiLine/>
    </ComplexToken>
    <ComplexToken>
      <Token pen="12">\R{</Token>
      <MatchToToken>}</MatchToToken>
      <MultiLine/>
    </ComplexToken>      
    <ComplexToken>
      <Token pen="2" bgpen="1">\label{</Token>
      <MatchToToken>}</MatchToToken>
      <Mutliline/>
    </ComplexToken>  
    <ComplexToken>
      <Token pen="2" bgpen="1">\ref{</Token>
      <MatchToToken>}</MatchToToken>
      <Mutliline/>
    </ComplexToken>  
    <ComplexToken>
      <Token pen="2" bgpen="1">\eqref{</Token>
      <MatchToToken>}</MatchToToken>
      <Mutliline/>
    </ComplexToken>  
    <ComplexToken>
      <Token pen="2" bgpen="1">\cite{</Token>
      <MatchToToken>}</MatchToToken>
      <Mutliline/>
    </ComplexToken>  
    <!--Preprocessing directives...-->
  </Syntax>

</Ann-SyntaxHighlighting>